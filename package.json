    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "antd": "4.19.1",
    "antd-img-crop": "^4.2.0",
    "crypto-js": "^4.1.1",
    "identity-obj-proxy": "^3.0.0",
    "izitoast": "^1.4.0",
    "izitoast-react": "^1.0.17",
    "jquery": "^3.6.0",
    "jsdom": "^19.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-markdown": "^8.0.1",
    "react-redux": "^7.2.6",
    "react-router-dom": "6",
    "react-scripts": "5.0.0",
